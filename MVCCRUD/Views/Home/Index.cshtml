@model DataAccessLibrary.DAL.Models.PeopleViewModel




<table class="table">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>City</th>
        <th>Actions</th>
    </tr>
    
    @foreach (var person in Model.People) 
    {
<tr>
    <td>@person.FirstName</td>
    <td>@person.LastName</td>
    <td>@person.Email</td>
    <td>@person.City</td>
    <td><a asp-controller="People" asp-action="EditPerson">Edit Person</a> </td>
    <td><a asp-controller="People" asp-action="DeletePerson">Delete Person</a> </td>
</tr>
    }


</table>



